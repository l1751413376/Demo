        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Beta"
        mc:Ignorable="d"
        Title="MainWindow" Width="810" Height="628" xmlns:Controls="clr-namespace:Beta.Controls" >
    <Window.Resources>
        <ResourceDictionary Source="Controls/Dictionary1.xaml"></ResourceDictionary>
    </Window.Resources>
    <Canvas x:Name="Carrier" Background="Silver" MouseDown="Carrier_MouseLeftButtonDown" Margin="1,0,1,1" Width="800" Height="600">
        <StackPanel Name="ExperiencePanel">
            <Rectangle x:Name="Experience" Canvas.ZIndex="0"></Rectangle>
        </StackPanel>
    </Canvas>

</Window>
